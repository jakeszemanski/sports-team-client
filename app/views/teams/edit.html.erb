<%= form_tag "/teams/#{@team["id"]}", method: :patch do %>
  <div class="form-group">
  <%= label_tag :state, 'Enter state name: ' %>
  <%= text_field_tag :state, @team['state'], class: 'form-control' %>
  </div>
   <div class="form-group">
  <%= label_tag :team, 'Enter team name: ' %>
  <%= text_field_tag :team, @team['team'], class: 'form-control' %>
  </div>
  <div class="form-group">
  <%= label_tag :city, 'Enter city: ' %>
  <%= text_field_tag :city, @team['city'], class: 'form-control' %>
  </div>
  <div>
  <%= submit_tag 'Submit team!' %>
  </div>
  <% end %>